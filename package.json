{
  "name": "plainspec-mocha-reporter",
  "version": "1.0.1",
  "description": "A simple mocha-reporter for console useful when testing async calls.",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/mocha/bin/_mocha --reporter index  test/*.js",
    "pretty":"prettier --write test/*.js index.js"
  },
  "keywords": [
    "mocha",
    "test",
    "unit",
    "reporter"
  ],
  "dependencies": {
    "printf": "0.5.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "5.2.0",
    "prettier": "1.15.3"
  },
  "author": "Bipul Kumar Kuri",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bipulkkuri/plainspec-mocha-reporter.git"
  },
  "bugs": {
    "url": "https://github.com/bipulkkuri/plainspec-mocha-reporter/issues"
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
