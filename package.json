{
  "name": "plainspec-mocha-reporter",
  "version": "1.0.10",
  "description": "A simple mocha-reporter for console useful when testing async calls.",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/mocha/bin/_mocha --reporter index   test/*.js --exit",
    "pretty": "prettier --write test/*.js index.js"
  },
  "keywords": [
    "mocha",
    "test",
    "unit",
    "reporter",
    "async"
  ],
  "dependencies": {
    "printf": ">=0.6.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "express": "4.19.2",
    "mocha": "^8.1.1",
    "morgan": "1.9.1",
    "prettier": "1.15.3"
  },
  "author": "Bipul Kumar Kuri",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bipulkkuri/plainspec-mocha-reporter.git"
  },
  "bugs": {
    "url": "https://github.com/bipulkkuri/plainspec-mocha-reporter/issues"
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
